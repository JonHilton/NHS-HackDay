@model List<NHS_HackDay.Common.Contact>

@{
  ViewBag.Title = "Index";
}

<h2>Directory</h2>

<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Person</th>
      <th>Extension</th>
      <th>Department</th>
    </tr>
  </thead>
  @foreach (var item in Model)
  {
    <tr>
      <td>@item.Name</td>
      <td>@item.Id</td>
      <td></td>
    </tr>
  }
</table>
